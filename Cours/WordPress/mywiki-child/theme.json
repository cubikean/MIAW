{
    "$schema": "https:\/\/schemas.wp.org\/trunk\/theme.json",
    "version": 2,
    "settings": {
        "appearanceTools": true,
        "color": {
            "custom": true,
            "customGradient": false,
            "link": false,
            "palette": [
                {
                    "name": "primary",
                    "slug": "primary",
                    "color": "#048eb0"
                },
                {
                    "name": "secondary",
                    "slug": "secondary",
                    "color": "#ff0000"
                }
            ]
        },
        "spacing": {
            "padding": false,
            "margin": false,
            "units": [
                "px",
                "em",
                "rem",
                "vh",
                "vw"
            ]
        },
        "typography": {
            "customFontSize": false,
            "customLineHeight": false,
            "dropCap": false,
            "fontFamilies": [
                []
            ]
        },
        "layout": {
            "contentSize": "840px",
            "wideSize": "1100px"
        }
    }
}